// ComplexNumber.java
class ComplexNumber {
    double real;
    double imag;

    // Constructor
    ComplexNumber(double real, double imag) {
        this.real = real;
        this.imag = imag;
    }

    // Method to add two complex numbers
    ComplexNumber add(ComplexNumber c) {
        return new ComplexNumber(this.real + c.real, this.imag + c.imag);
    }

    // Method to subtract two complex numbers
    ComplexNumber subtract(ComplexNumber c) {
        return new ComplexNumber(this.real - c.real, this.imag - c.imag);
    }

    // Method to multiply two complex numbers
    ComplexNumber multiply(ComplexNumber c) {
        double realPart = (this.real * c.real) - (this.imag * c.imag);
        double imagPart = (this.real * c.imag) + (this.imag * c.real);
        return new ComplexNumber(realPart, imagPart);
    }

    // Method to display the complex number
    void display() {
        if (imag >= 0)
            System.out.println(real + " + " + imag + "i");
        else
            System.out.println(real + " - " + (-imag) + "i");
    }
}

// Main class
